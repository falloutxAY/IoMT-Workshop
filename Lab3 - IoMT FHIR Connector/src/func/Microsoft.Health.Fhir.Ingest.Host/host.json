{
  "aggregator": {
    "batchSize": 5000,
    "flushTimeout": "00:01:00"
  },
  "logging": {
    "applicationInsights": {
      "enableDependencyTracking": true,
      "enablePerformanceCountersCollection": true,
      "samplingSettings": {
        "isEnabled": true,
        "maxTelemetryItemsPerSecond": 20
      }
    }
  },
  "extensions": {
    "eventHubs": {
      "batchCheckpointFrequency": 1,
      "eventProcessorOptions": {
        "maxBatchSize": 1024,
        "prefetchCount": 2048
      }
    }
  },
  "version": "2.0"
}